<?xml version='1.0' encoding='utf-8'?>
<widget id="com.tti.ttiep" version="1.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ttiContact</name>
    <description>
        ttiContact
    </description>
    <author email="ep_notifiy@tti.tv" href="http://www.tti.tv">
        tti-ep
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
